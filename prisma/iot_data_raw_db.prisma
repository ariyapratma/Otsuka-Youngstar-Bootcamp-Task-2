generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/iot_data_raw_db"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_IOT_DATA_RAW")
}

model steam_boiler_12tph {
  DateTime      DateTime  @id @db.DateTime(0)
  Value         Float?    @db.Float
  Consume       Float?    @db.Float
  Unit          String?   @db.VarChar(20)
  StartDateTime DateTime? @db.DateTime(0)
  IsTotalizer   Int?

  @@map("86_steam_boiler_12tph")
}

model alarm_system_results {
  id            Int            @id @default(autoincrement())
  value         Float?         @db.Float
  severity      String?        @db.VarChar(255)
  alarmSystemId Int?
  createdAt     DateTime       @db.DateTime(0)
  updatedAt     DateTime       @db.DateTime(0)
  remark        String?        @db.VarChar(255)
  alarm_systems alarm_systems? @relation(fields: [alarmSystemId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "alarm_system_results_ibfk_1")

  @@index([alarmSystemId], map: "alarmSystemId")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model alarm_systems {
  id                   Int                      @id @default(autoincrement())
  endpoint             String?                  @db.VarChar(255)
  nodeId               String?                  @db.VarChar(255)
  status               String?                  @db.VarChar(255)
  isBoolean            Boolean?
  standardCriticalMin  Float?                   @db.Float
  standardCriticalMax  Float?                   @db.Float
  standardHighMin      Float?                   @db.Float
  standardHighMax      Float?                   @db.Float
  standardMediumMin    Float?                   @db.Float
  standardMediumMax    Float?                   @db.Float
  standardLowMin       Float?                   @db.Float
  standardLowMax       Float?                   @db.Float
  createdAt            DateTime                 @db.DateTime(0)
  updatedAt            DateTime                 @db.DateTime(0)
  deviceDescription    String?                  @db.VarChar(255)
  valueType            alarm_systems_valueType? @default(max)
  machineStatusTag     String?                  @db.VarChar(255)
  alarm_system_results alarm_system_results[]
}

